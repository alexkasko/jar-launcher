# macosx environment file example
# to build run these conmmands:
# . macosx.env
# make -e

# platform name
export JL_PLATFORM=macosx

# compile settings
export JL_COMPILER_OPTIONS="-Os -std=gnu99"

# result executable name
export JL_EXECUTABLE_NAME=launcher

# paths are relative to executable file
# use forward slashes only
export JL_JAVA_RELATIVE_PATH=jre/bin/java
export JL_JAR_RELATIVE_PATH=launcher.jar
